#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/ulid/bin/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/ulid/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules:/home/ubuntu/repos/sns_app_from_devin/node_modules:/home/ubuntu/repos/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/ulid/bin/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/ulid/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules:/home/ubuntu/repos/sns_app_from_devin/node_modules:/home/ubuntu/repos/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/repos/sns_app_from_devin/vue-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ulid/bin/cli.js" "$@"
else
  exec node  "$basedir/../ulid/bin/cli.js" "$@"
fi
